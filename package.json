{
  "name": "webpack-boilerplate",
  "version": "1.1.0",
  "description": "Webpack 4 boilerplate: Babel, TypeScript, Sass, hot reload, and dev server",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "lint": "tslint -p tsconfig.json -c tslint.json && stylelint src/**/*.scss",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/webpack-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "scss",
    "primitive-ui",
    "boilerplate",
    "css",
    "typescript"
  ],
  "author": "Tania Rascia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taniarascia/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/taniarascia/webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/d3": "^5.7.0",
    "@types/d3-selection": "^1.4.1",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.12.0",
    "postcss-loader": "^7.0.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.4",
    "ts-loader": "^9.4.2",
    "tslint": "^5.12.1",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.4.1"
  },
  "dependencies": {
    "d3": "^5.9.2"
  }
